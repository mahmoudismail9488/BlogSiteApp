<%-include("/header")-%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label class="form-label" for = "postTitle">Title</label>
        <input class="form-control" autocomplete="off" type="text" name="postTitle">
    </div>
    <div class="form-group">
        <label class="form-label" for="myPost">Post</label>
        <textarea class="form-control" autocomplete="off"  name = "myPost" rows="10" ></textarea>
    </div>
    
    <button  class="btn btn-primary" type="submit" name = "post_btn" >Publish</button>
</form>
<%-include("/footer")-%>